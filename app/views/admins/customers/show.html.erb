<h2>会員詳細画面</h2>
 <div class="form-group row">
  <div>
   <label for="inputId">会員ID</label>
   <%= @customer.admin.id %>
  </div>
 </div>

 <div class="form-group row">
  <div>
   <label for="inputName">氏名</label>
   <%= @customer.admin.last_name %>
   <%= @customer.admin.first_name %>
  </div>
 </div>

 <div class="form-group row">
  <div>
   <label for="inputName_kana">フリガナ</label>
   <%= @customer.adminlast_name_kana %>
   <%= @customer.admin.first_name_kana %>
  </div>
 </div>

 <div class="form-group row">
  <div>
   <label for="inputPostal_code">郵便番号</label>
   <%= @customer.admin.postal_code %>
  </div>
 </div>

 <div class="form-group row">
  <div>
   <label for="inputAddress">住所</label>
   <%= @customer.admin.address %>
  </div>
 </div>

 <div class="form-group row">
  <div>
   <label for="inputTelephone_number">電話番号</label>
   <%= @customer.admin.telephone_number %>
  </div>
 </div>

 <div class="form-group row">
  <div>
   <label for="inputEmail">メールアドレス</label>
   <%= @customer.admin.email %>
  </div>
 </div>

 <div class="form-group row">
  <div>
   <label for="inputIs_active">会員ステータス</label>
   <%= @customer.admin.is_active %>
  </div>
 </div>

 <%= link_to "編集する", edit_admins_customer_path(customer) %>
 <%= link_to "注文履歴一覧を見る", admins_path %>